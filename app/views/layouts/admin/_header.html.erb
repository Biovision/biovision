<header class="main-header">
  <%= render 'layouts/admin/header/logo' %>
  <nav>
    <% unless current_user.nil? %>
      <ul>
        <li><%= current_user.screen_name %></li>
        <li>
          <%=
            link_to(
                image_tag('biovision/icons/log_out.svg', alt: t(:log_out)),
                logout_path, method: :delete
            )
          %>
        </li>
      </ul>
    <% end %>
  </nav>
</header>
