<div class="data">
  <div><%= admin_entity_link(entity) %></div>
  <div class="info"><%= entity.slug %></div>
  <div class="secondary info">
    <%= t(:page_count, count: entity.dynamic_pages_count) %>
  </div>

  <ul class="actions">
    <li><%= edit_icon(edit_navigation_group_path(id: entity.id)) %></li>
    <li class="danger"><%= destroy_icon(@entity) %></li>
  </ul>
</div>
