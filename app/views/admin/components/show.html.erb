<% content_for :meta_title, t('.title', slug: @handler.slug) %>
<% content_for :breadcrumbs do %>
  <%= link_to(t('admin.components.index.nav_text'), admin_components_path) %>
  <span><%= @handler.name %></span>
<% end %>

<article>
  <%=
    render(
        partial: 'admin/components/entity/section',
        locals: { component: @handler.component, handler: @handler }
    )
  %>
</article>
